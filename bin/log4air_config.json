{
  "appenders": [
    {
      "name": "STDOUT",
      "class": "net.blaxstar.log4air.appenders.ConsoleAppender",
      "layout": {
        "class": "net.blaxstar.log4air.layouts.PatternLayout",
        "pattern": "%date%level%class %msg"
      }
    },
    {
      "name": "FILE",
      "class": "net.blaxstar.log4air.appenders.FileAppender",
      "layout": {
        "class": "net.blaxstar.log4air.layouts.PatternLayout",
        "pattern": "%date%level%class %msg"
      },
      "filepath": "/Users/dyxribo/Desktop/log4air.log"
    }
  ],
  "loggers": [
    {
      "name": "STDOUT",
      "level": "DEBUG",
      "type": "STDOUT"
    },
    {
      "name": "LOGFILE",
      "level": "DEBUG",
      "type": "FILE"
    }
  ],
  "root": {
    "level": "OFF",
    "type": "STDOUT"
  }
}
